
<div class="container">
  <div class="row">
    <h3>This is the index page</h3>
  </div>
  <div class="row">
    <% @videos.each do |video| %>
      <div class="col-md-4">
        <div class="thumbnail">
          <%= link_to song_path(video.song) do %>
            <img src="#{video.thumb_url}" alt="A video thumbnail" height="280" width="320">
          <% end %>
        </div>
      </div>
    <% end %>
  </div>
</div>
