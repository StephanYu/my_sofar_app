
<div class="container">
  <div class="row">
    <% @videos.in_groups_of(3, false) do |group| %>
      <% group.each do |video| %>
        <div class="col-md-4">
          <div class="thumbnail">
            <%= video.thumbnail %>
          </div>
        </div>
      <% end %>
    <% end %>
  </div>
  <div class="row">
    <div class="center">
      <div><%= will_paginate @videos, renderer: BootstrapPagination::Rails %></div>
    </div>
  </div>
</div>
